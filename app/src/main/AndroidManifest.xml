<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="rs.aleph.android.fearpally">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="rs.aleph.android.fearpally.pripremni.activity.ListActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:icon="@mipmap/ic_launcher"
            android:label="Actors">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="rs.aleph.android.fearpally.pripremni.activity.Detail"
            android:icon="@mipmap/ic_launcher"
            android:label="Actor Detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="rs.aleph.android.fearpally.pripremni.preferences.PripremaPrefererences"
            android:label="@string/activity_settings_name"
            android:parentActivityName="rs.aleph.android.fearpally.pripremni.activity.ListActivity"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>